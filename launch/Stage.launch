<launch>
    <master auto="start"/>
    <param name="/use_sim_time" value="true"/>
    <include file="$(find navigation_stage)/move_base_config/move_base.xml"/>
    <node name="map_server" pkg="map_server" type="map_server" args="$(find Gesture-Localisation-Robot)/data/map.pgm 0.025" />
    <node pkg="stage_ros" type="stageros" name="stageros" args="$(find Gesture-Localisation-Robot)/data/cafe2.world" respawn="false" >
    <param name="base_watchdog_timeout" value="0.2"/>
  </node>
    <include file="$(find navigation_stage)/move_base_config/amcl_node.xml"/>
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find Gesture-Localisation-Robot)/demo.rviz"/>
<!--     <node name="slam" pkg="gmapping" type="slam_gmapping" args="scan:=base_scan"> -->
<!--          --><!-- To receive less warnings but get less publications. See: http://wiki.ros.org/gmapping--> -->
<!--         <param name="transform_publish_period" value="0.08"/> -->
<!--     </node> -->
</launch>